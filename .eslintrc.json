{
  "extends": ["eslint:recommended", "prettier", "plugin:node/recommended"],

  "rules": {
    "no-console": 1
  },
  "plugins": ["import", "node"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
